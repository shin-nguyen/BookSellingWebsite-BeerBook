                                    <li>@Html.ActionLink("Shop", "Shop", "HomeScreen")</li>
                                    <li>@Html.ActionLink("About", "About", "HomeScreen")</li>
                                    <li>@Html.ActionLink("Contact", "Contact", "HomeScreen")</li>
                                </ul>
                            </nav>
                        </div>
                        <!-- Header Bottom Menu Area End Here -->
                    </div>
                </div>
            </div>
        </div>
        <!-- Header Bottom Area End Here -->
        <!-- Begin Mobile Menu Area -->
        <div class="mobile-menu-area d-lg-none d-xl-none col-12">
            <div class="container">
                <div class="row">
                    <div class="mobile-menu">
